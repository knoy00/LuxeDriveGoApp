{
  "expo": {
    "name": "luxedrivegoapp",
    "slug": "luxedrivegoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist" : {
        "NSFaceIDUsageDescription": "Allow ${luxedrivegoapp} to use Face ID."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-local-authentication",
        {
          "android": "Allow ${luxedrivegoapp} to use Face ID."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "firebaseApiKey": "AIzaSyC8pSlMmzqFsptXrACdyhtcKfqRJmiViXY",
      "firebaseAuthDomain": "luxedrivego.firebaseapp.com",
      "firebaseProjectId": "luxedrivego",
      "firebaseStorageBucket": "luxedrivego.firebasestorage.app",
      "firebaseMessagingSenderId": "703719786559",
      "firebaseAppId": "1:703719786559:web:9446461a1a8b13b08a908a",
      "firebaseMeasurementId": "G-8E93N716TF"
    }
  }
}
